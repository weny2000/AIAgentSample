{
  "name": "ai-agent-infrastructure",
  "version": "1.0.0",
  "description": "Infrastructure as Code for AI Agent System",
  "private": true,
  "scripts": {
    "build": "tsc",
    "deploy": "cdk deploy",
    "deploy:dev": "cdk deploy --all --context stage=dev",
    "deploy:staging": "cdk deploy --all --context stage=staging --require-approval never",
    "deploy:production": "cdk deploy --all --context stage=production",
    "destroy": "cdk destroy",
    "diff": "cdk diff",
    "diff:dev": "cdk diff --context stage=dev",
    "diff:staging": "cdk diff --context stage=staging",
    "diff:production": "cdk diff --context stage=production",
    "synth": "cdk synth",
    "test": "jest",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "migrate": "ts-node src/scripts/database-migration.ts",
    "migrate:dev": "ts-node src/scripts/database-migration.ts dev",
    "migrate:staging": "ts-node src/scripts/database-migration.ts staging",
    "migrate:production": "ts-node src/scripts/database-migration.ts production",
    "migrate:dry-run": "ts-node src/scripts/database-migration.ts dev --dry-run",
    "config:deploy": "ts-node src/scripts/environment-config-manager.ts deploy",
    "config:retrieve": "ts-node src/scripts/environment-config-manager.ts retrieve",
    "config:validate": "ts-node src/scripts/environment-config-manager.ts validate",
    "config:delete": "ts-node src/scripts/environment-config-manager.ts delete",
    "blue-green:deploy": "ts-node src/scripts/blue-green-deployment.ts deploy",
    "blue-green:rollback": "ts-node src/scripts/blue-green-deployment.ts rollback"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.899.0",
    "@aws-sdk/util-dynamodb": "^3.899.0",
    "aws-cdk-lib": "^2.90.0",
    "aws-jwt-verify": "^5.1.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.0",
    "aws-cdk": "^2.90.0",
    "jest": "^29.6.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.2"
  }
}
